{
  "name": "slacker775/flysystem-backblaze-b2",
  "description": "Flysystem adapter for Backblaze B2 storage",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "David Hollis",
      "email": "slacker775@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Slacker775\\Flysystem\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Slacker775\\Flysystem\\Tests\\": "tests"
    }
  },
  "require-dev": {
    "league/flysystem-adapter-test-utilities": "^2.0|^3.0",
    "symfony/http-client": "^6.0",
    "symfony/var-dumper": "^6.0",
    "php-http/httplug": "^2.2",
    "phpunit/phpunit": "^10.0"
  },
  "require": {
    "league/flysystem": "^2.0|^3.0",
    "slacker775/backblaze-b2-api": "^0.2.0",
    "guzzlehttp/psr7": "^2.1",
    "nyholm/psr7": "^1.4"
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": true
    }
  }
}
